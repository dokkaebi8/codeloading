javascript:(function(){if(window.myBookmarkletRunning){return;}window.myBookmarkletRunning=true;if(document.getElementById('custom-console')){return;}var%20c=document.createElement('div');c.id='custom-console';c.style.cssText='position:fixed;top:0;left:0;right:0;background:rgba(0,0,0,0.9);color:#00ff00;padding:15px;z-index:99999;font-family:"Courier%20New",monospace;max-height:200px;overflow-y:auto;font-size:14px;line-height:1.5;border-bottom:2px%20solid%20#00ff00;box-shadow:0%200%2010px%20rgba(0,255,0,0.3);backdrop-filter:blur(5px);';document.body.appendChild(c);function%20log(m,t='info'){var%20line=document.createElement('div');var%20ts=new%20Date().toLocaleTimeString('ja-JP',{hour12:false});var%20color='#00ff00';var%20prefix='[INFO]';if(t==='error'){color='#ff0000';prefix='[ERROR]';}else%20if(t==='warn'){color='#ffff00';prefix='[WARN]';}else%20if(t==='success'){color='#00ff00';prefix='[OK]';}line.style.cssText='color:'+color+';margin:3px%200;text-shadow:0%200%205px%20'+color+';';line.innerHTML='<span%20style="color:#888;">'+ts+'</span>'+prefix+'%20'+m;c.insertBefore(line,c.firstChild);if(c.children.length>8){c.removeChild(c.lastChild);}}function%20handleCode(data){try{const%20parts=data.trim().split('|');if(parts.length===2){const%20dt=parseInt(parts[0]);const%20td=Math.abs(Date.now()-dt);log('Time%20difference:'+td+'ms','info');if(td<=3000){const%20m=parts[1].match(/drop=([^&]+)/);if(m&&m[1]){const%20dc=m[1];log('Code%20detected:'+dc,'success');var%20inp=document.evaluate('//*[@id="main-content"]/div/div[2]/div/div/div/div[2]/div[2]/div/div/section[2]/form/div[1]/div/label/div/div[1]/input',document,null,XPathResult.FIRST_ORDERED_NODE_TYPE,null).singleNodeValue;if(inp){inp.focus();inp.value='';for(var%20i=0;i<dc.length;i++){var%20char=dc[i];var%20eventOptions={key:char,keyCode:char.charCodeAt(0),which:char.charCodeAt(0),bubbles:true};inp.dispatchEvent(new%20KeyboardEvent('keydown',eventOptions));inp.dispatchEvent(new%20KeyboardEvent('keypress',eventOptions));inp.value+=char;inp.dispatchEvent(new%20Event('input',{bubbles:true}));inp.dispatchEvent(new%20KeyboardEvent('keyup',eventOptions));}log('Input%20updated','success');setTimeout(function(){var%20btn=document.evaluate('//*[@id="main-content"]/div/div[2]/div/div/div/div[2]/div[2]/div/div/section[2]/form/div[2]/button',document,null,XPathResult.FIRST_ORDERED_NODE_TYPE,null).singleNodeValue;if(btn){btn.click();log('Button%20clicked','success');}else{log('Button%20not%20found','error');}},150);}}}}}catch(e){log(e.message,'error');}}const%20eventSource=new%20EventSource('https://polished-hat-2c2d.t29323285.workers.dev/?url=http://hanuchain.noor.jp/codeserver.php');eventSource.onmessage=function(event){handleCode(event.data);};eventSource.onerror=function(){log('Connection%20error.%20Reconnecting...','error');};log('SYSTEM%20INITIALIZED','success');})();
